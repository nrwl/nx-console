{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "out-tsc/jest",
    "types": ["jest", "node"],
    "module": "commonjs",
    "moduleResolution": "node10"
  },
  "include": [
    "jest.config.ts",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/*.test.tsx",
    "**/*.spec.tsx",
    "**/*.test.js",
    "**/*.spec.js",
    "**/*.test.jsx",
    "**/*.spec.jsx",
    "**/*.d.ts",
    "../../../types/html.d.ts"
  ],
  "references": [{ "path": "./tsconfig.lib.json" }]
}
