{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "nxui": {
      "root": "apps/nxui/",
      "sourceRoot": "apps/nxui/src",
      "projectType": "application",
      "prefix": "apps/nxui",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/nxui",
            "index": "apps/nxui/src/index.html",
            "main": "apps/nxui/src/main.ts",
            "polyfills": "apps/nxui/src/polyfills.ts",
            "tsConfig": "apps/nxui/tsconfig.app.json",
            "assets": ["apps/nxui/src/favicon.ico", "apps/nxui/src/assets"],
            "styles": ["apps/nxui/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/nxui/src/environments/environment.ts",
                  "with": "apps/nxui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "nxui:build",
            "proxyConfig": "apps/nxui/proxy.conf.js"
          },
          "configurations": {
            "production": {
              "browserTarget": "nxui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "nxui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/nxui/src/test.ts",
            "polyfills": "apps/nxui/src/polyfills.ts",
            "tsConfig": "apps/nxui/tsconfig.spec.json",
            "karmaConfig": "apps/nxui/karma.conf.js",
            "styles": ["apps/nxui/src/styles.scss"],
            "scripts": [],
            "assets": ["apps/nxui/src/favicon.ico", "apps/nxui/src/assets"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/nxui/tsconfig.app.json",
              "apps/nxui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "nxui-e2e": {
      "root": "apps/nxui-e2e/",
      "sourceRoot": "apps/nxui-e2e/src",
      "projectType": "application"
    },
    "feature-workspaces": {
      "root": "libs/feature-workspaces",
      "sourceRoot": "libs/feature-workspaces/src",
      "projectType": "library",
      "prefix": "nxui",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/feature-workspaces/src/test.ts",
            "tsConfig": "libs/feature-workspaces/tsconfig.spec.json",
            "karmaConfig": "libs/feature-workspaces/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/feature-workspaces/tsconfig.lib.json",
              "libs/feature-workspaces/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "feature-extensions": {
      "root": "libs/feature-extensions",
      "sourceRoot": "libs/feature-extensions/src",
      "projectType": "library",
      "prefix": "nxui",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/feature-extensions/src/test.ts",
            "tsConfig": "libs/feature-extensions/tsconfig.spec.json",
            "karmaConfig": "libs/feature-extensions/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/feature-extensions/tsconfig.lib.json",
              "libs/feature-extensions/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "feature-generate": {
      "root": "libs/feature-generate",
      "sourceRoot": "libs/feature-generate/src",
      "projectType": "library",
      "prefix": "nxui",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/feature-generate/src/test.ts",
            "tsConfig": "libs/feature-generate/tsconfig.spec.json",
            "karmaConfig": "libs/feature-generate/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/feature-generate/tsconfig.lib.json",
              "libs/feature-generate/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "utils": {
      "root": "libs/utils",
      "sourceRoot": "libs/utils/src",
      "projectType": "library",
      "prefix": "nxui",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/utils/src/test.ts",
            "tsConfig": "libs/utils/tsconfig.spec.json",
            "karmaConfig": "libs/utils/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/utils/tsconfig.lib.json",
              "libs/utils/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "feature-run": {
      "root": "libs/feature-run",
      "sourceRoot": "libs/feature-run/src",
      "projectType": "library",
      "prefix": "nxui",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/feature-run/src/test.ts",
            "tsConfig": "libs/feature-run/tsconfig.spec.json",
            "karmaConfig": "libs/feature-run/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/feature-run/tsconfig.lib.json",
              "libs/feature-run/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "ui": {
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "projectType": "library",
      "prefix": "nxlibs/ui",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/ui/src/test.ts",
            "tsConfig": "libs/ui/tsconfig.spec.json",
            "karmaConfig": "libs/ui/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/ui/tsconfig.lib.json",
              "libs/ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics"
  },
  "defaultProject": "nxui"
}
