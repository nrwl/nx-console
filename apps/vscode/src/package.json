{
  "name": "angular-console",
  "displayName": "Angular Console",
  "description": "Angular Console for Visual Studio Code. The user interface app for the Angular CLI",
  "version": "8.0.11",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/angular-console"
  },
  "author": {
    "name": "Narwhal Technologies Inc",
    "email": "hello@nrwl.io"
  },
  "publisher": "nrwl",
  "main": "main.js",
  "icon": "assets/extension_icon.png",
  "galleryBanner": {
    "color": "#176BCC",
    "theme": "dark"
  },
  "homepage": "https://angularconsole.com/",
  "bugs": {
    "url": "https://github.com/nrwl/angular-console/issues"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.31.0"
  },
  "keywords": [
    "Angular",
    "Angular 2",
    "Angular 8",
    "CLI",
    "Console"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "dependencies": {
    "jsonc-parser": "^2.1.1"
  },
  "contributes": {
    "commands": [
      {
        "category": "ng",
        "title": "lint",
        "command": "ng-console.lint"
      },
      {
        "category": "ng",
        "title": "build",
        "command": "ng-console.build"
      },
      {
        "category": "ng",
        "title": "deploy",
        "command": "ng-console.deploy"
      },
      {
        "category": "ng",
        "title": "e2e",
        "command": "ng-console.e2e"
      },
      {
        "category": "ng",
        "title": "serve",
        "command": "ng-console.serve"
      },
      {
        "category": "ng",
        "title": "test",
        "command": "ng-console.test"
      },
      {
        "category": "ng",
        "title": "xi18n",
        "command": "ng-console.xi18n"
      }
    ],
    "problemMatchers": [
      {
        "name": "webpack-builder",
        "label": "TypeScript + Webpack Problem Matcher",
        "owner": "typescript",
        "source": "ts",
        "applyTo": "closedDocuments",
        "fileLocation": "absolute",
        "severity": "error",
        "pattern": [
          {
            "regexp": "\\[tsl\\] ERROR in (.*)?\\((\\d+),(\\d+)\\)",
            "file": 1,
            "line": 2,
            "column": 3
          },
          {
            "regexp": "\\s*TS\\d+:\\s*(.*)",
            "message": 1
          }
        ]
      }
    ],
    "taskDefinitions": [
      {
        "type": "ng",
        "required": [
          "task"
        ],
        "properties": {
          "projectName": {
            "type": "string",
            "description": "The Angular CLI project to run a command on"
          },
          "architectName": {
            "type": "string",
            "description": "The name of the architect command to run on the selected project"
          }
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "angular-console",
          "title": "Angular Console",
          "icon": "tree-view-icon.svg"
        }
      ]
    },
    "views": {
      "angular-console": [
        {
          "id": "angularConsole",
          "name": "Navigation"
        },
        {
          "id": "angularConsoleJson",
          "name": "Angular JSON"
        }
      ]
    }
  }
}
