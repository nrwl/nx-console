{
  "sourceRoot": "apps/vscode/src",
  "projectType": "application",
  "prefix": "vscode",
  "generators": {},
  "implicitDependencies": [
    "vscode-ui",
    "nxls"
  ],
  "targets": {
    "watch-all": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "yarn nx watch vscode",
            "prefix": "vscode",
            "color": "blue"
          },
          {
            "command": "yarn nx watch vscode-ui",
            "prefix": "vscode-ui",
            "color": "green"
          },
          {
            "command": "yarn nx watch nxls",
            "prefix": "lsp",
            "color": "magenta"
          }
        ],
        "parallel": true
      }
    },
    "watch": {
      "executor": "@nrwl/esbuild:esbuild",
      "options": {
        "external": [
          "vscode",
          "typescript"
        ],
        "outputPath": "dist/apps/vscode",
        "main": "apps/vscode/src/main.ts",
        "tsConfig": "apps/vscode/tsconfig.app.json",
        "assets": [
          {
            "input": "./",
            "glob": "package.json",
            "output": "/"
          },
          {
            "input": "./",
            "glob": "nx-schema.json",
            "output": "/"
          },
          {
            "input": "./",
            "glob": "tree-view-icon.svg",
            "output": "/"
          },
          {
            "input": "./",
            "glob": "assets",
            "output": "/"
          },
          {
            "input": "./",
            "glob": "getting-started",
            "output": "/"
          },
          {
            "glob": "README.md",
            "input": "./",
            "output": "/"
          },
          {
            "glob": "CHANGELOG.md",
            "input": "./",
            "output": "/"
          },
          {
            "glob": "LICENSE",
            "input": "./",
            "output": "/"
          }
        ],
        "watch": true
      }
    },
    "build": {
      "executor": "@nrwl/esbuild:esbuild",
      "defaultConfiguration": "production",
      "options": {
        "esbuildOptions": {
          "legalComments": "inline"
        },
        "external": [
          "vscode",
          "typescript"
        ],
        "outputPath": "dist/apps/vscode",
        "main": "apps/vscode/src/main.ts",
        "tsConfig": "apps/vscode/tsconfig.app.json",
        "buildLibsFromSource": true,
        "deleteOutputPath": false,
        "assets": [
          {
            "input": "apps/vscode/src",
            "glob": "package.json",
            "output": "/"
          },
          {
            "input": "apps/vscode/src",
            "glob": "nx-schema.json",
            "output": "/"
          },
          {
            "input": "apps/vscode/src",
            "glob": "tree-view-icon.svg",
            "output": "/"
          },
          {
            "input": "apps/vscode/src",
            "glob": "assets",
            "output": "/"
          },
          {
            "input": "apps/vscode/src",
            "glob": "getting-started",
            "output": "/"
          },
          {
            "glob": "README.md",
            "input": "./",
            "output": "/"
          },
          {
            "glob": "CHANGELOG.md",
            "input": "./",
            "output": "/"
          },
          {
            "glob": "LICENSE",
            "input": "./",
            "output": "/"
          }
        ]
      },
      "configurations": {
        "ci": {
          "optimization": false
        },
        "production": {
          "sourceMap": false,
          "optimization": true,
          "extractLicenses": true,
          "external": [
            "vscode",
            "jsonc-parser",
            "@monodon/typescript-nx-imports-plugin",
            "@yarnpkg/fslib",
            "@yarnpkg/libzip",
            "@swc/core",
            "ts-node"
          ],
          "fileReplacements": [
            {
              "replace": "apps/vscode/src/environments/environment.ts",
              "with": "apps/vscode/src/environments/environment.prod.ts"
            }
          ]
        }
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/vscode/**/*.ts"
        ]
      }
    },
    "package": {
      "executor": "@nrwl/workspace:run-commands",
      "dependsOn": [
        "build",
        "^build"
      ],
      "options": {
        "commands": [
          "node ./tools/scripts/vscode-yarn.js",
          "node ./tools/scripts/vscode-vsce.js"
        ],
        "parallel": false
      },
      "outputs": [
        "dist/apps/vscode"
      ]
    }
  },
  "tags": []
}
