{
  "implicitDependencies": {
    "package.json": {
      "devDependencies": "*",
      "dependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "master"
  },
  "npmScope": "nx-console",
  "useDaemonProcess": true,
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook",
          "package",
          "build-webview-files",
          "extract-dependencies"
        ],
        "accessToken": "YTg3Yzk1YWMtZTQ3MC00MTIwLTkyM2ItMjAyMGI0ZTJlZDc3fHJlYWQtd3JpdGU="
      }
    }
  },
  "cli": {
    "packageManager": "yarn"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "generate-ui",
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {},
  "namedInputs": {
    "nodeModules": ["{workspaceRoot}/node_modules/"],
    "kotlin": [
      "{projectRoot}/build.gradle.kts",
      "{projectRoot}/**/*.kt",
      "{workspaceRoot}/**/gradle.properties",
      "{workspaceRoot}/settings.gradle.kts",
      "{workspaceRoot}/build.gradle.kts"
    ]
  }
}
