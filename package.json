{
  "name": "angular-console",
  "version": "7.2.0-rc.0",
  "private": true,
  "publisher": "nrwl",
  "author": {
    "name": "Narwhal Technologies Inc",
    "email": "hello@nrwl.io"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.29.1"
  },
  "scripts": {
    "nx": "nx",
    "ng": "ng",
    "start": "nps",
    "format": "nps format.write",
    "build-vs-code": "ng build vscode",
    "postinstall": "node ./tools/scripts/postinstall.js"
  },
  "build": {
    "appId": "io.nrwl.AngularConsole",
    "productName": "Angular Console",
    "copyright": "Copyright Â© 2018 Narwhal Technologies Inc",
    "asar": false,
    "extraResources": [
      {
        "from": "dist/apps/electron/assets",
        "to": "app/assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "removePackageScripts": false,
    "generateUpdatesFilesForAllChannels": true,
    "directories": {
      "app": "dist/apps/electron",
      "buildResources": "dist/apps/electron/assets/build",
      "output": "dist/packages"
    },
    "npmRebuild": true,
    "mac": {
      "category": "public.app-category.developer-tools",
      "artifactName": "AngularConsole-${version}.${ext}"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb",
        "tar.gz"
      ],
      "synopsis": "Angular Console",
      "category": "Development"
    },
    "nsis": {
      "createDesktopShortcut": "always",
      "installerIcon": "icons/icon.ico",
      "artifactName": "AngularConsole-Setup-${version}.${ext}"
    },
    "afterPack": "./tools/scripts/after-pack.js",
    "publish": [
      {
        "provider": "github",
        "owner": "nrwl",
        "repo": "angular-console"
      }
    ]
  },
  "dependencies": {
    "@nrwl/nx": "7.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.11.4",
    "@angular-devkit/build-ng-packagr": "0.11.4",
    "@angular-devkit/core": "7.2.0-rc.0",
    "@angular-devkit/schematics": "7.2.0-rc.0",
    "@angular/animations": "7.2.0",
    "@angular/cdk": "7.2.1",
    "@angular/cli": "7.2.0-rc.0",
    "@angular/common": "7.2.0",
    "@angular/compiler-cli": "7.2.0",
    "@angular/compiler": "7.2.0",
    "@angular/core": "7.2.0",
    "@angular/flex-layout": "7.0.0-beta.22",
    "@angular/forms": "7.2.0",
    "@angular/language-service": "7.2.0",
    "@angular/material": "7.2.1",
    "@angular/platform-browser-dynamic": "7.2.0",
    "@angular/platform-browser": "7.2.0",
    "@angular/router": "7.2.0",
    "@graphql-modules/logger": "^0.2.3",
    "@nrwl/angular-console-enterprise-electron": "0.0.38",
    "@nrwl/angular-console-enterprise-frontend": "0.0.38",
    "@nrwl/builders": "7.3.0",
    "@nrwl/schematics": "7.3.0",
    "@types/electron-store": "^1.3.0",
    "@types/express": "4.16.0",
    "@types/fontfaceobserver": "^0.0.6",
    "@types/get-port": "^4.0.1",
    "@types/graphql": "14.0.3",
    "@types/jest": "^23.3.10",
    "@types/node": "8.10.38",
    "@types/opn": "^5.1.0",
    "@types/request": "^2.48.1",
    "@types/semver": "^5.5.0",
    "@types/shelljs": "^0.8.1",
    "@types/sinon-chai": "3.2.2",
    "@types/sinon": "7.0.3",
    "@types/strip-json-comments": "^0.0.30",
    "@types/tmp": "^0.0.33",
    "@types/universal-analytics": "0.4.2",
    "apollo-angular-link-http": "1.4.0",
    "apollo-angular": "1.5.0",
    "apollo-cache-inmemory": "1.3.12",
    "apollo-link-error": "1.1.5",
    "apollo-link": "1.2.4",
    "apollo-server-express": "^2.2.2",
    "codelyzer": "4.5.0",
    "core-js": "2.5.7",
    "cypress": "^3.1.4",
    "dotenv": "6.2.0",
    "electron-builder": "20.28.4",
    "electron-installer-dmg": "2.0.0",
    "electron-packager": "13.0.1",
    "electron-rebuild": "^1.8.2",
    "electron-store": "2.0.0",
    "electron-updater": "3.1.2",
    "electron": "2.0.14",
    "express": "4.16.4",
    "fix-path": "2.1.0",
    "fontfaceobserver": "^2.1.0",
    "get-port": "4.0.0",
    "graphql-code-generator": "0.15.2",
    "graphql-codegen-typescript-apollo-angular": "0.15.2",
    "graphql-codegen-typescript-client": "0.15.2",
    "graphql-codegen-typescript-common": "0.15.2",
    "graphql-codegen-typescript-resolvers": "0.15.2",
    "graphql-codegen-typescript-server": "0.15.2",
    "graphql-tag": "2.10.0",
    "graphql": "^14.0.2",
    "jasmine-marbles": "0.4.1",
    "jest-jasmine2": "23.6.0",
    "jest-preset-angular": "6.0.2",
    "jest": "^23.6.0",
    "ng-packagr": "4.4.5",
    "node-pty-prebuilt": "0.7.6",
    "nps-utils": "1.7.0",
    "nps": "5.9.3",
    "opn": "^5.4.0",
    "prettier": "1.15.3",
    "progress-stream": "2.0.0",
    "request": "2.88.0",
    "resize-observer-polyfill": "^1.5.1",
    "resolve": "1.9.0",
    "rxjs-compat": "6.3.3",
    "rxjs": "6.3.3",
    "shelljs": "0.8.3",
    "strip-json-comments": "2.0.1",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tsickle": "0.34.0",
    "tslib": "1.9.3",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "tslint": "5.12.0",
    "typescript": "3.1.6",
    "universal-analytics": "^0.4.20",
    "uuid": "^3.3.2",
    "vsce": "1.54.0",
    "vscode": "^1.1.26",
    "xterm": "3.9.1",
    "zone.js": "0.8.26"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "dist",
      "tmp"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testURL": "http://localhost",
    "testMatch": [
      "**/*.+(spec|test).(ts|js)"
    ]
  }
}
