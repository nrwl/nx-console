{
  "name": "angular-console",
  "version": "0.0.1",
  "private": true,
  "publisher": "nrwl",
  "author": {
    "name": "Narwhal Technologies Inc",
    "email": "hello@nrwl.io"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.35.0"
  },
  "scripts": {
    "nx": "nx",
    "ng": "ng",
    "start": "nps",
    "format": "nps format.write",
    "build-vs-code": "nps prepare.dev.vscode",
    "postinstall": "node ./tools/scripts/postinstall.js",
    "update": "ng update @nrwl/workspace"
  },
  "build": {
    "appId": "io.nrwl.AngularConsole",
    "productName": "Angular Console",
    "copyright": "Copyright Â© 2018 Narwhal Technologies Inc",
    "asar": false,
    "files": [
      "**/*"
    ],
    "extraResources": [
      {
        "from": "dist/apps/electron/assets",
        "to": "app/assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "removePackageScripts": false,
    "generateUpdatesFilesForAllChannels": true,
    "directories": {
      "app": "dist/apps/electron",
      "buildResources": "dist/apps/electron/assets/build",
      "output": "dist/packages"
    },
    "npmRebuild": true,
    "mac": {
      "category": "public.app-category.developer-tools",
      "artifactName": "AngularConsole-${version}.${ext}",
      "icon": "icons/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "icons/icon.ico"
    },
    "linux": {
      "target": [
        "deb",
        "tar.gz"
      ],
      "artifactName": "AngularConsole-${version}.${ext}",
      "synopsis": "Angular Console",
      "category": "Development",
      "icon": "icons/icon.png"
    },
    "nsis": {
      "createDesktopShortcut": "always",
      "installerIcon": "icons/icon.ico",
      "artifactName": "AngularConsole-Setup-${version}.${ext}"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "nrwl",
        "repo": "angular-console"
      }
    ]
  },
  "dependencies": {
    "@angular/core": "8.1.2",
    "@nrwl/angular": "8.2.0",
    "@types/fontfaceobserver": "^0.0.6",
    "core-js": "2.6.9",
    "fontfaceobserver": "^2.1.0",
    "ij-rpc-client": "^0.3.2",
    "node-pty": "^0.8.1",
    "pretty-quick": "^1.11.1",
    "source-map-explorer": "^1.8.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "tmp": "^0.1.0",
    "webpack": "^4.36.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.801.2",
    "@angular-devkit/build-ng-packagr": "0.801.2",
    "@angular/animations": "8.1.2",
    "@angular/cdk": "8.1.1",
    "@angular/cli": "8.1.2",
    "@angular/common": "8.1.2",
    "@angular/compiler": "8.1.2",
    "@angular/compiler-cli": "8.1.2",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "8.1.2",
    "@angular/language-service": "8.1.2",
    "@angular/material": "8.1.1",
    "@angular/platform-browser": "8.1.2",
    "@angular/platform-browser-dynamic": "8.1.2",
    "@angular/router": "8.1.2",
    "@graphql-modules/logger": "^0.2.3",
    "@nestjs/common": "5.7.0",
    "@nestjs/core": "5.7.0",
    "@nestjs/graphql": "5.5.1",
    "@nrwl/angular-console-enterprise-electron": "^0.3.686",
    "@nrwl/angular-console-enterprise-frontend": "^0.3.686",
    "@nrwl/cypress": "8.2.0",
    "@nrwl/jest": "8.2.0",
    "@nrwl/node": "8.2.0",
    "@nrwl/workspace": "8.2.0",
    "@types/d3-hierarchy": "^1.1.6",
    "@types/d3-selection": "^1.4.1",
    "@types/d3-shape": "^1.3.1",
    "@types/d3-transition": "^1.1.4",
    "@types/electron-store": "^3.2.0",
    "@types/graphql": "14.0.5",
    "@types/jest": "24.0.9",
    "@types/node": "11.13.0",
    "@types/opn": "^5.1.0",
    "@types/request": "^2.48.1",
    "@types/semver": "^5.5.0",
    "@types/shelljs": "^0.8.2",
    "@types/sinon": "7.0.13",
    "@types/sinon-chai": "3.2.2",
    "@types/strip-json-comments": "^0.0.30",
    "@types/tmp": "^0.0.33",
    "@types/universal-analytics": "0.4.2",
    "apollo-angular": "1.6.0",
    "apollo-angular-link-http": "1.8.0",
    "apollo-cache-inmemory": "1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "1.2.12",
    "apollo-link-error": "1.1.11",
    "apollo-server-express": "^2.6.9",
    "codelyzer": "5.1.0",
    "cypress": "3.4.0",
    "d3-hierarchy": "^1.1.8",
    "d3-selection": "^1.4.0",
    "d3-shape": "^1.3.4",
    "d3-transition": "^1.2.0",
    "dotenv": "6.2.0",
    "electron": "4.2.6",
    "electron-builder": "21.1.1",
    "electron-installer-dmg": "3.0.0",
    "electron-packager": "14.0.2",
    "electron-rebuild": "^1.8.5",
    "electron-store": "3.3.0",
    "electron-updater": "4.1.2",
    "express": "4.17.1",
    "fast-glob": "3.0.4",
    "fix-path": "2.1.0",
    "get-port": "5.0.0",
    "graphql": "14.4.2",
    "graphql-code-generator": "0.18.2",
    "graphql-codegen-typescript-apollo-angular": "0.18.2",
    "graphql-codegen-typescript-client": "0.18.2",
    "graphql-codegen-typescript-common": "0.18.2",
    "graphql-codegen-typescript-resolvers": "0.18.2",
    "graphql-codegen-typescript-server": "0.18.2",
    "graphql-tag": "2.10.1",
    "graphql-tools": "4.0.5",
    "husky": "^1.3.1",
    "jasmine-marbles": "^0.6.0",
    "jest": "24.1.0",
    "jest-jasmine2": "23.6.0",
    "jest-preset-angular": "7.0.0",
    "ng-packagr": "5.2.0",
    "nps": "5.9.3",
    "nps-utils": "1.7.0",
    "opn": "^5.4.0",
    "prettier": "1.16.4",
    "progress-stream": "2.0.0",
    "reflect-metadata": "0.1.13",
    "request": "2.88.0",
    "resize-observer-polyfill": "^1.5.1",
    "resolve": "1.10.0",
    "rxjs": "6.5.2",
    "rxjs-compat": "6.4.0",
    "shelljs": "0.8.3",
    "shx": "^0.3.2",
    "strip-json-comments": "2.0.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.0.2",
    "tsickle": "0.35.0",
    "tslib": "1.9.3",
    "tslint": "5.12.1",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "3.4.5",
    "universal-analytics": "0.4.20",
    "uuid": "3.3.2",
    "vsce": "^1.66.0",
    "vscode": "^1.1.35",
    "xterm": "3.14.5",
    "zlib": "^1.0.5",
    "zone.js": "0.9.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "dist",
      "tmp"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testURL": "http://localhost",
    "testMatch": [
      "**/*.+(spec|test).(ts|js)"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && git add ."
    }
  }
}
