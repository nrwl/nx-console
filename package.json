{
  "name": "angular-console",
  "version": "6.0.0-beta.1",
  "private": true,
  "author": "Narwhal Technologies Inc",
  "license": "MIT",
  "scripts": {
    "start": "nps",
    "format": "nps format.write",
    "postinstall": "node ./tools/scripts/postinstall.js"
  },
  "build": {
    "appId": "io.nrwl.AngularConsole",
    "productName": "Angular Console",
    "copyright": "Copyright Â© 2018 Narwhal Technologies Inc",
    "asar": false,
    "directories": {
      "app": "dist/electron",
      "buildResources": "dist/electron/assets/icons/build",
      "output": "dist/packages"
    },
    "npmRebuild": true,
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "nsis": {
      "createDesktopShortcut": "always",
      "installerIcon": "icon.ico"
    },
    "afterPack": "./tools/scripts/after-pack.js",
    "publish": [
      {
        "provider": "github",
        "owner": "nrwl",
        "repo": "angular-console"
      }
    ]
  },
  "dependencies": {
    "@nrwl/nx": "6.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular-devkit/core": "^0.6.8",
    "@angular-devkit/schematics": "0.6.8",
    "@angular/animations": "^6.0.7",
    "@angular/cdk": "^6.4.1",
    "@angular/cli": "6.0.8",
    "@angular/common": "6.0.7",
    "@angular/compiler": "6.0.7",
    "@angular/compiler-cli": "6.0.7",
    "@angular/core": "6.0.7",
    "@angular/flex-layout": "^6.0.0-beta.16",
    "@angular/forms": "6.0.7",
    "@angular/language-service": "6.0.7",
    "@angular/material": "^6.4.1",
    "@angular/platform-browser": "6.0.7",
    "@angular/platform-browser-dynamic": "6.0.7",
    "@angular/router": "6.0.7",
    "@nrwl/schematics": "6.1.0",
    "@types/express": "^4.16.0",
    "@types/graphql": "0.13.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "8.10.21",
    "@types/sinon": "4.3.3",
    "@types/sinon-chai": "2.7.31",
    "angular-playground": "5.0.1",
    "apollo-angular": "^1.1.2",
    "apollo-angular-link-http": "^1.1.1",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "1.1.0",
    "codelyzer": "~4.4.2",
    "core-js": "^2.5.7",
    "cypress": "3.0.3",
    "electron": "2.0.5",
    "electron-builder": "20.19.2",
    "electron-installer-dmg": "^1.0.0",
    "electron-packager": "12.1.0",
    "express": "^4.16.3",
