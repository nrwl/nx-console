{
  "name": "angular-console",
  "version": "7.0.0",
  "private": true,
  "author": {
    "name": "Narwhal Technologies Inc",
    "email": "hello@nrwl.io"
  },
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "ng": "ng",
    "start": "nps",
    "format": "nps format.write",
    "postinstall": "node ./tools/scripts/postinstall.js"
  },
  "build": {
    "appId": "io.nrwl.AngularConsole",
    "productName": "Angular Console",
    "copyright": "Copyright Â© 2018 Narwhal Technologies Inc",
    "asar": false,
    "extraResources": [
      {
        "from": "dist/server/assets",
        "to": "app/assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "removePackageScripts": false,
    "directories": {
      "app": "dist/server",
      "buildResources": "dist/server/assets/icons/build",
      "output": "dist/packages"
    },
    "npmRebuild": true,
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "tar.gz"
      ],
      "synopsis": "Angular Console",
      "category": "Development"
    },
    "nsis": {
      "createDesktopShortcut": "always",
      "installerIcon": "icon.ico",
      "artifactName": "AngularConsole-Setup-${version}.${ext}"
    },
    "afterPack": "./tools/scripts/after-pack.js",
    "publish": [
      {
        "provider": "github",
        "owner": "nrwl",
        "repo": "angular-console"
      }
    ]
  },
  "dependencies": {
    "@graphql-modules/logger": "^0.2.0",
    "@nrwl/nx": "7.0.2",
    "@types/strip-json-comments": "^0.0.30",
    "karma-spec-reporter": "^0.0.32",
    "karma-verbose-reporter": "^0.0.6",
    "request": "2.88.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.10.6",
    "@angular-devkit/build-ng-packagr": "0.10.6",
    "@angular-devkit/core": "0.8.7",
    "@angular-devkit/schematics": "0.8.7",
    "@angular/animations": "7.0.4",
    "@angular/cdk": "7.0.4",
    "@angular/cli": "7.0.4",
    "@angular/common": "7.0.4",
    "@angular/compiler": "7.0.4",
    "@angular/compiler-cli": "7.0.4",
    "@angular/core": "7.0.4",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "7.0.4",
    "@angular/language-service": "7.0.4",
    "@angular/material": "7.0.4",
    "@angular/platform-browser": "7.0.4",
    "@angular/platform-browser-dynamic": "7.0.4",
    "@angular/router": "7.0.4",
    "@nrwl/angular-console-enterprise-electron": "0.0.16",
    "@nrwl/angular-console-enterprise-frontend": "0.0.16",
    "@nrwl/schematics": "7.0.2",
    "@types/express": "4.16.0",
    "@types/graphql": "0.13.4",
    "@types/jasmine": "2.8.11",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "8.10.38",
    "@types/sinon": "4.3.3",
    "@types/sinon-chai": "2.7.33",
    "@types/universal-analytics": "0.4.2",
    "angular-playground": "5.1.5",
    "apollo-angular": "1.5.0",
    "apollo-angular-link-http": "1.4.0",
    "apollo-cache-inmemory": "1.3.9",
    "apollo-client": "2.4.5",
    "apollo-link": "1.2.3",
    "apollo-link-error": "1.1.1",
    "apollo-server-express": "2.1.0",
    "codelyzer": "4.5.0",
    "core-js": "2.5.7",
    "cypress": "3.1.1",
    "drivelist": "6.3.0",
    "electron": "2.0.13",
    "electron-builder": "20.28.4",
    "electron-installer-dmg": "1.0.0",
    "electron-packager": "12.2.0",
    "electron-store": "2.0.0",
    "electron-updater": "3.1.2",
    "express": "4.16.4",
    "express-graphql": "0.6.12",
    "fix-path": "2.1.0",
    "get-port": "3.2.0",
    "graphql": "0.13.2",
    "graphql-code-generator": "0.12.6",
    "graphql-codegen-apollo-angular-template": "0.12.6",
    "graphql-codegen-typescript-template": "0.12.6",
    "graphql-tag": "2.10.0",
    "jasmine-core": "3.2.1",
    "jasmine-marbles": "0.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "23.4.2",
    "jest-jasmine2": "23.6.0",
    "karma": "2.0.5",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.3.1",
    "ng-packagr": "4.4.0",
    "node-pty-prebuilt": "0.7.3",
    "nps": "5.9.3",
    "nps-utils": "1.7.0",
    "prettier": "1.15.1",
    "protractor": "5.3.0",
    "resolve": "1.8.1",
    "rxjs": "6.3.3",
    "rxjs-compat": "6.2.2",
    "shelljs": "0.8.2",
    "strip-json-comments": "2.0.1",
    "ts-jest": "23.10.4",
    "ts-node": "7.0.1",
    "tsickle": "0.33.1",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "typescript": "3.1.3",
    "universal-analytics": "0.4.17",
    "uuid": "3.3.2",
    "xterm": "3.4.1",
    "zone.js": "0.8.26",
    "@types/jquery": "3.3.22"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "dist",
      "tmp"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testURL": "http://localhost",
    "testMatch": [
      "**/*.+(spec|test).(ts|js)"
    ]
  }
}
